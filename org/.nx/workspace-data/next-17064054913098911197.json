{
  "10402066019117660914": {
    "build": {
      "command": "next build",
      "options": {
        "cwd": "apps/nextlearn",
        "tty": false
      },
      "dependsOn": [
        "^build"
      ],
      "cache": true,
      "inputs": [
        "default",
        "^production",
        {
          "externalDependencies": [
            "next"
          ]
        }
      ],
      "outputs": [
        "{workspaceRoot}/apps/nextlearn/.next/!(cache)/**/*",
        "{workspaceRoot}/apps/nextlearn/.next/!(cache)"
      ],
      "syncGenerators": [
        "@nx/js:typescript-sync"
      ]
    },
    "dev": {
      "continuous": true,
      "command": "next dev",
      "options": {
        "cwd": "apps/nextlearn"
      },
      "syncGenerators": [
        "@nx/js:typescript-sync"
      ]
    },
    "start": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/nextlearn"
      },
      "dependsOn": [
        "build"
      ]
    },
    "serve-static": {
      "continuous": true,
      "command": "next start",
      "options": {
        "cwd": "apps/nextlearn"
      },
      "dependsOn": [
        "build"
      ]
    },
    "build-deps": {
      "dependsOn": [
        "^build"
      ]
    },
    "watch-deps": {
      "continuous": true,
      "dependsOn": [
        "build-deps"
      ],
      "command": "npx nx watch --projects @org/nextlearn --includeDependentProjects -- npx nx build-deps @org/nextlearn"
    }
  },
